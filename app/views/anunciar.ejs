  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>
  <link href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" rel="stylesheet" />
</head>

<body style="background-color: lightblue;">

  <!--Corpo da página HTML-->
  <header>
    <!--cabeçalho da página-->
    <nav class="text-center">
      <!--aplicação de uma classe-->

      <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
          <a href="/">Home</a>
          <a class="select" href="/anunciar">Anuncie uma vaga</a>
          <a href="/contato">Fale Conosco</a>
        </div>
      </div>
    </nav>
  </header>
  <br>
  <div class="container">
    <div class="row">
      <div class="col-md-12 col-sm-12 col-xs-12">
      </div>
    </div>
    <br>




    <h1>Formulario para inclusão de vagas</h1>
    <form action="/empregos/salvar" method="post">
      <input type="text" name="titulo" placeholder="Titulo da vaga">
      <input type="number" name="salário" placeholder="Digite o salário">
      <input type="text" name="descricao" placeholder="Digite a descrição da vaga">
      <input type="text" name="local" placeholder="Digite a cidade e estado da vaga">
      <input type="text" name="empresa" placeholder="Digite o nome da empresa">
      <input type="text" name="area" placeholder="Digite a area da vaga">
      <input type="submit" value="enviar">
    </form>




  </div>




  <br>
  <footer class="footer">
